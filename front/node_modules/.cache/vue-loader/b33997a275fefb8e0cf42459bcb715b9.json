<div class=\"flexCenter add-file-container\">\r\n                <input type=\"file\" name=\"file\" id=\"file\" accept=\"image/*\" @change=\"preview()\" ref=\"addFile\">\r\n                <button class=\"btn add-image-btn\" @click=\"$refs.addFile.click()\" type=\"button\">Ajouter une image</button>\r\n                <button class=\"btn green-btn publish-btn\" @click.prevent=\"postArticle()\" type=\"submit\">Publier</button>\r\n            </div>\r\n\r\n            <p class=\"flexCenter form-alert\" v-if=\"isEmpty\">{{ emptyMsg }}</p>\r\n        </form>\r\n\r\n    </section>\r\n</template>\r\n\r\n\r\n<script>\r\n    import UserCaption from \"../UserCaption.vue\"\r\n    import routesAPI from \"../../../routesAPI.js\"\r\n\r\n    export default {\r\n        name: \"Publish\",\r\n\r\n        mixins: [\r\n            routesAPI,\r\n        ],\r\n\r\n        components: {\r\n            UserCaption,            \r\n        },\r\n\r\n        data(){\r\n            return {\r\n                file: \"\",\r\n                isPublish: false,\r\n                isEmpty: false,\r\n            }\r\n        },\r\n\r\n        methods: {\r\n            preview() {\r\n                const file = document.getElementById(\"file\").files;\r\n\r\n                if(file.length > 0) {\r\n                    const fileReader = new FileReader();\r\n                    fileReader.onload = (event) => document.querySelector(\".imagePreview\").setAttribute(\"src\", event.target.result);\r\n                    fileReader.readAsDataURL(file[0]);\r\n                }\r\n\r\n                this.file = this.$refs.addFile.files[0];\r\n            },\r\n\r\n            postArticle() {\r\n                const title = document.getElementById(\"title\").value;\r\n                const textContent = document.getElementById(\"textContent\").value;\r\n\r\n                const postForm = document.querySelector(\".post-form\");\r\n                let formData = new FormData(postForm);\r\n\r\n                formData.set(\"file\", this.file);\r\n                formData.forEach((key, value) => formData[value] = key);\r\n\r\n                if(title !== \"\" && textContent !== \"\" || this.file ) {\r\n                    this.createPublish_API(formData);    \r\n                    this.$emit(\"posted\", this.isPublish);\r\n                }\r\n\r\n                else {\r\n                    this.isEmpty = true;\r\n                    this.emptyMsg = \"Vous devez Ã©crire du text ou importer une image !\";\r\n                    setTimeout(() => this.isEmpty = false , 3000);\r\n                }\r\n\r\n            },\r\n        }\r\n    }\r\n</script>\r\n\r\n\r\n<style scoped>\r\n    .publish-flow {\r\n        overflow: hidden;\r\n        align-items: flex-start;\r\n        height: 90%; \r\n    }\r\n\r\n    .post-form {        \r\n        position: relative;\r\n        height: auto;\r\n        border-radius: 20px;\r\n    }\r\n\r\n    /* ========== Alert message ========== */\r\n    .form-alert {\r\n        position: absolute;\r\n        height: 40px;\r\n        width: 80%;\r\n        margin: 0;\r\n        padding: 5px;\r\n        bottom: 35px;\r\n        font-size: 100%;\r\n        line-height: 110%;\r\n    }\r\n\r\n    .post-form,\r\n    .user-pict {\r\n        margin: 8px;\r\n    }\r\n\r\n    .post-container {\r\n        justify-content: space-between;\r\n        width: 90%;\r\n    }\r\n\r\n    .post-container textarea {\r\n        margin-top: 5px;\r\n        height: 50px;\r\n    }\r\n\r\n    .add-file-container input[type=file] {\r\n        display: none;\r\n    }\r\n\r\n\r\n    /* ========== File Preview ==========*/\r\n    .imagePreview {\r\n        margin: auto;\r\n        margin-top: 5px;\r\n        object-fit: cover;\r\n        width: 50%;\r\n        max-height: 120px;\r\n        max-width: 150px;\r\n    }\r\n\r\n\r\n    /* ========== Buttons ========== */\r\n    .publish-btn {\r\n        width: 50%;\r\n    }\r\n\r\n    .add-image-btn,\r\n    .publish-btn {\r\n        margin-top: 8px;\r\n    }\r\n\r\n    .add-image-btn {\r\n        width: 72%;\r\n    }\r\n\r\n    .publish-btn {\r\n        margin-bottom: 10px;\r\n    }\r\n</style>"]}]}